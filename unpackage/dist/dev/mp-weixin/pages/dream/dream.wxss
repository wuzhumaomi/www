.allBox {
  padding: 20rpx;
  min-height: 100vh;
  background-color: #eadac0;
}
.allBox .inputBox .u-label {
  -webkit-box-flex: 0 !important;
  -webkit-flex: 0 0 0 !important;
          flex: 0 0 0 !important;
}
.allBox .inputBox button {
  height: 80rpx;
}
.allBox .inputBox input, .allBox .inputBox button {
  border: 1rpx solid #d3ad76;
  box-shadow: 0 0 8rpx #d3ad76;
  background-color: #e6caa3;
  padding: 20rpx;
  border-radius: 20rpx;
  color: #666666;
}
.allBox .list {
  margin-top: 10rpx;
  border: 1rpx solid #d3ad76;
  box-shadow: 0 0 8rpx #d3ad76;
  border-radius: 6rpx;
  background-color: #e6caa3;
}
.allBox .btnText, .allBox .topText {
  margin: 10rpx;
  color: #666666;
}
.allBox .topText {
  color: #606266;
  font-size: 36rpx;
  font-weight: 600;
}
.allBox .btn {
  width: 33%;
  margin: 0.15%;
  display: inline-block;
  color: #FFFFFF;
}

